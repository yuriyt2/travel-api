<h2>Destinations</h2>
<ul>
<%@destinations.each do |d|%>
<li style="display:inline-block;margin-right:10px;">
 <h5 style="width:180"> <%=d.name%> </h5>
 <a href="<%=edit_destination_path(d)%>"><%=image_tag d.img_url.split(",")[0], :style => "width:180px;max-height:120px;"%></a>
</li>
 <%end%>
</ul>
